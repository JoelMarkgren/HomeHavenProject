@inject IResidenceService ResidenceService
@foreach (var Resi in Residences)
{
	<ul>
		<li>
			<h2>@Resi.MainPicture @Resi.Address @Resi.ResidenceCategory.CategoryName</h2>
		</li>
	</ul>
}




@code {
	[Parameter]
	public List<ResidenceDto>? DisplayResidences { get; set; }

	public IEnumerable<ResidenceDto>? Residences { get; set; }

	protected override async Task OnInitializedAsync()
	{
		Residences = await ResidenceService.GetAllAsync();
	}
}